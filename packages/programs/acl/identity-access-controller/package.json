{
	"name": "@peerbit/identity-access-controller",
	"version": "5.0.78",
	"description": "Access controller that operates on a DB",
	"sideEffects": false,
	"type": "module",
	"main": "./dist/src/index.js",
	"types": "./dist/src/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			],
			"src/*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			]
		}
	},
	"files": [
		"dist/src",
		"!dist/src/**/*.test.js",
		"!dist/src/**/*.spec.js",
		"!**/*.tsbuildinfo",
		"LICENSE",
		"README.md"
	],
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
			"import": "./dist/src/index.js"
		},
		"./access": {
			"types": "./dist/src/access.d.ts",
			"import": "./dist/src/access.js"
		},
		"./acl-db": {
			"types": "./dist/src/acl-db.d.ts",
			"import": "./dist/src/acl-db.js"
		},
		"./condition": {
			"types": "./dist/src/condition.d.ts",
			"import": "./dist/src/condition.js"
		}
	},
	"eslintConfig": {
		"extends": "peerbit",
		"parserOptions": {
			"project": true,
			"sourceType": "module"
		},
		"ignorePatterns": [
			"!.aegir.js",
			"test/ts-use",
			"*.d.ts"
		]
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"clean": "aegir clean",
		"build": "aegir build --no-bundle",
		"test": "aegir test --target node",
		"lint": "aegir lint"
	},
	"author": "dao.xyz",
	"license": "MIT",
	"dependencies": {
		"@dao-xyz/borsh": "^5.2.3",
		"@peerbit/document": "9.6.2",
		"@peerbit/trusted-network": "4.1.95",
		"reflect-metadata": "^0.2.2",
		"tslib": "^2.8.1"
	},
	"devDependencies": {
		"@peerbit/test-utils": "^2.1.46",
		"@peerbit/time": "^2.1.0",
		"@types/chai": "^5.2.2",
		"@types/chai-as-promised": "^8.0.2",
		"@types/mocha": "^10.0.10",
		"chai": "^5.2.0",
		"chai-as-promised": "^8.0.1"
	}
}
